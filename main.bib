  author        = {Besson, Adrien and Zhang, Miaomiao and Varray, Francois and Liebgott, Herve and Friboulet, Denis and Wiaux, Yves and Thiran, Jean Philippe and Carrillo, Rafael E. and Bernard, Olivier},
