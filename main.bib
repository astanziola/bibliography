month = {aug},
number = {8},
pages = {5189--5195},
title = {{The infinite planar baffles problem in acoustic radiation and its experimental verification}},
url = {https://aip.scitation.org/doi/10.1063/1.326656},
volume = {50},
year = {1979}
}

