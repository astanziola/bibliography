@Article{Ito2015,
  author        = {Ito, Koichi and Noro, Kazumasa and Yanagisawa, Yukari and Sakamoto, Maya and Mori, Shiro and Shiga, Kiyoto and Kodama, Tetsuya and Aoki, Takafumi},
